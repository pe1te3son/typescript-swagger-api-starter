{
  "name": "typescript-micro-services-starter",
  "version": "1.0.0",
  "description": "node.js typescript starter",
  "main": "index.js",
  "scripts": {
    "build:prod": "npm run tslint && npm run clean:dist && tsc && npm run copy",
    "copy": "npm run copy-assets",
    "copy-assets": "copyfiles -u 1 \"./src/**/*.json\" \"./dist/\"",
    "copy-pck-json": "copyfiles \"./package*.json\" \"./dist/\"",
    "clean:dist": "rimraf dist",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "start": "nodemon -V --exec ts-node src/server.ts",
    "start:debug": "nodemon -V --exec DEBUG=* ts-node src/server.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "keywords": [
    "typesript",
    "node"
  ],
  "author": "Peter Janak",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.11.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "config": "^3.0.1",
    "deep-extend": "^0.6.0",
    "express": "^4.16.4",
    "json-schema-ref-parser": "^6.0.3",
    "lodash": "^4.17.19",
    "morgan": "^1.9.1",
    "swagger-node-runner": "^0.7.3",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/config": "0.0.34",
    "@types/debug": "0.0.31",
    "@types/express": "^4.16.0",
    "@types/json-schema": "^7.0.1",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.18",
    "@types/pino": "^5.8.3",
    "@types/swagger-node-runner": "^0.6.0",
    "@types/swagger-ui-express": "^4.1.1",
    "copy-webpack-plugin": "^5.1.1",
    "copyfiles": "^2.1.0",
    "debug": "^4.1.1",
    "nodemon": "^2.0.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.3.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.2.4",
    "webpack": "^4.41.5"
  }
}
